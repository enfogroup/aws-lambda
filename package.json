{
  "name": "@enfo/rename-me",
  "version": "0.0.1",
  "description": "A fleshed out typescript NPM package template",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx husky install",
    "prepublishOnly": "npx pinst --disable",
    "postpublish": "npx pinst --enable",
    "publish:package": "cd .dist && npm publish",
    "build": "npx webpack",
    "test": "npx jest --runInBand",
    "lint": "npx eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "npx eslint --fix 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enfogroup/rename-me.git"
  },
  "keywords": [],
  "author": "Urban Pettersson",
  "license": "MIT",
  "homepage": "https://github.com/enfogroup/rename-me#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "copy-webpack-plugin": "^9.0.1",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.8",
    "jest": "^27.2.4",
    "pinst": "^2.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {}
}
